<h3>Patient List</h3>
<h2><a href="#/">New Patient</a></h2>

<div class="row main">
	<div class="col-xl-12">
	<table class="table table-striped table-hover">
		<thead>
			<tr>
				<th>Name</th>
				<th>Phone Number</th>
				<th>Appointment Date</th>
				<th>Reason</th>
				<th>Status</th>
				<th>Dentist</th>
			</tr>
		</thead>
		<tbody>
			<tr ng-repeat="patient in patients">
				<td ng-bind="patient.name"></td>
				<td ng-bind="patient.phone"></td>
				<td ng-bind="patient.appointment"></td>
				<td ng-bind="patient.reason"></td>
				<td ng-bind="patient.status"></td>
				<td ng-bind="patient.dentist"></td>
				<td><button><a href="#/update/{{patient._id}}">Update</a></button></td>
			</tr>
		</tbody>
	</table>
	</div>
</div>